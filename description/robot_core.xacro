<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"  name="robot">

<material name="white">
        <color rgba="1 1 1 1" />
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1" />
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1" />
    </material>

    <material name="black">
        <color rgba="0 0 0 1" />
    </material>

    <material name="red">
        <color rgba="1 0 0 1" />
    </material>



    <link name="base_link">
        
    </link>

    <joint name="chassis_joiny" type="revolute">
        <parent link="base_link"/>
        <child link="chassis"/>
    </joint>
    
    <link name="chassis">
        <visual>
            <origin xyz="0.15 0.0 0.0"/>
            <geometry>
                <box size="0.1 0.2 0.2"/>
            </geometry>
        </visual>
    </link>

</robot>